    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "prettier": "^1.18.2",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-multi-entry": "^2.1.0",
    "sucrase": "^3.10.1"
