{
  "name": "rollup-plugin-flow-entry",
  "version": "0.3.2",
  "private": false,
  "description": "Allows Flow to find the original typed source code for the Rollup bundle",
  "keywords": [
    "rollup-plugin"
  ],
  "license": "MIT",
  "author": "William Swanson <william@edge.app>",
  "files": [
    "CHANGELOG.md",
    "lib/*",
    "package.json",
    "README.md",
    "src/*"
  ],
  "main": "lib/index.js",
